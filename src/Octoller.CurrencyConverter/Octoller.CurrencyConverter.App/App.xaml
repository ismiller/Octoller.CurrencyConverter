<Application
    x:Class="Octoller.CurrencyConverter.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Octoller.CurrencyConverter.App"
    xmlns:locator="using:Octoller.CurrencyConverter.App.Infrastructure"
    xmlns:converter="using:Octoller.CurrencyConverter.App.Infrastructure.Converters">
    
    <Application.Resources>
        <ResourceDictionary>
            <locator:ServiceLocator x:Key="sLocator"/>
            <converter:ValuteConverter x:Key="vConverter"/>
            <converter:DecimalConverter x:Key="dConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
